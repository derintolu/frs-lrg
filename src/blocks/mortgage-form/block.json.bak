{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "lrh/mortgage-form",
  "title": "Mortgage Application Form",
  "category": "frs-blocks",
  "icon": "clipboard",
  "description": "Multi-step mortgage application form with Interactivity API",
  "supports": {
    "html": false,
    "interactivity": true
  },
  "attributes": {
    "pageType": {
      "type": "string",
      "default": "loan-app"
    }
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "viewScriptModule": "frs-mortgage-landing-interactivity"
}
