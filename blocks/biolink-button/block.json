{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "lrh/biolink-button",
	"version": "1.0.0",
	"title": "Biolink Button",
	"category": "frs-blocks",
	"description": "Interactive button for biolink pages",
	"keywords": ["biolink", "button", "cta"],
	"textdomain": "lending-resource-hub",
	"editorScript": "file:./edit.js",
	"attributes": {
		"text": {
			"type": "string",
			"default": "Button Text"
		},
		"url": {
			"type": "string",
			"default": "#"
		},
		"icon": {
			"type": "string",
			"default": "link"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"textColor": {
			"type": "string",
			"default": "#000000"
		},
		"isPrimary": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"customClassName": false
	},
	"example": {}
}